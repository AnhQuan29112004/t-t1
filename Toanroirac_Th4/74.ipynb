   ],
   "source": [
    "from sympy import *\n",
    "\n",
    "n = symbols('n')\n",
    "a = symbols('a', cls=Function)\n",
    "rsolve(\n",
    "    -a(n) + 3*a(n-2) - 2*a(n-3),\n",
    "    a(n)\n",
    ")\n",
    "\n"
