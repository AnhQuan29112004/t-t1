   "source": [
    "from sympy import *\n",
    "\n",
    "x = symbols('x')\n",
    "P = x**3 - 3*x + 2\n",
    "P.factor()\n",
    "\n",
    "C1, C2, C3 = symbols('C1 C2 C3')\n",
    "n = symbols('n')\n",
    "ans = C1 + C2*n + (-2)**n * C3\n",
    "\n",
    "[ans.subs(n, i) for i in [0, 1, 2]]\n",
    "\n",
    "eqns = [ans.subs(n, i) - a for i, a in zip([0, 1, 2], [5, -1, 2])]\n",
    "solve(eqns)\n"
   ]
